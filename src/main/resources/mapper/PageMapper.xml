<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.als.Beans.Blogs.mapper.PageMapper">
    <select id="selectById" parameterType="object" resultType="com.als.Beans.Blogs.entity.Pages">
        SELECT *
        FROM pages
        WHERE id = #{id}
    </select>
</mapper>